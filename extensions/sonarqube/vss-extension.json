{
  "manifestVersion": 1,
  "id": "devonfw-sonarqube",
  "name": "devonfw-SonarQube",
  "version": "0.0.6",
  "branding": {
    "color": "rgb(67, 157, 210)",
    "theme": "dark"
  },
  "publisher": "ADC-devonfw",
  "homepage": "https://www.sonarqube.org/",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    },
    {
      "id": "Microsoft.TeamFoundation.Server",
      "version": "[15.2,)"
    }
  ],
  "description":
    "Detect bugs, vulnerabilities and code smells across project branches and pull requests.",
  "public": false,
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "extension-icon.png"
  },
  "tags": ["build", "ci", "continuous integration", "code quality", "code analysis", "SAST", "security"],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license-terms.md"
    }
  },
  "links": {
    "home": {
      "uri": "https://www.sonarqube.org/"
    },
    "getstarted": {
      "uri": "http://redirect.sonarsource.com/doc/install-configure-scanner-tfs-ts.html"
    },
    "support": {
      "uri": "https://community.sonarsource.com/"
    },
    "privacypolicy": {
      "uri": "https://www.sonarqube.org/privacy/"
    }
  },
  "CustomerQnASupport": {
    "enableqna": "true",
    "url": "https://github.com/Jorge-Dacal/"
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/Jorge-Dacal/sonar-scanner-vsts/tree/custom/myplugin/publicSonar"
  },
  "files": [
    {
      "path": "tasks"
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "icons",
      "addressable": true
    }
  ],
  "scopes": ["vso.build", "vso.code", "vso.test"],
  "contributions": [
    {
      "id": "9F57024B-31F9-4E58-9E39-A47CCC098D5A",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/scanner-cli"
      }
    },
    {
      "id": "15B84CA1-B62F-4A2A-A403-89B77A063D5A",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/prepare"
      }
    },
    {
      "id": "6D01813A-9589-4B15-8491-8164AEB38D5A",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/analyze"
      }
    },
    {
      "id": "291ed61f-1ee4-45d3-b1b0-bf822d909d5a",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/publish"
      }
    },
    {
      "id": "EDC775DD-1A82-4ADD-A073-7B5813E87D5A",
      "description": "Service Endpoint type for SonarQube Connections",
      "type": "ms.vss-endpoint.service-endpoint-type",
      "targets": ["ms.vss-endpoint.endpoint-types"],
      "properties": {
        "name": "devonfw-sonarqube",
        "displayName": "devonfw SonarQube",
        "icon": "icons/devonfw-sonar.png",
        "url": {
          "displayName": "Server Url",
          "helpText": "Url for the SonarQube Server to connect to."
        },
        "authenticationSchemes": [
          {
            "type": "ms.vss-endpoint.endpoint-auth-scheme-basic",
            "inputDescriptors": [
              {
                "id": "username",
                "name": "Token",
                "description":
                  "Authentication Token generated through SonarQube (go to My Account > Security > Generate Tokens)",
                "inputMode": "passwordbox",
                "isConfidential": true,
                "validation": {
                  "isRequired": true,
                  "dataType": "string"
                }
              },
              {
                "id": "password",
                "name": "Password",
                "description": "Password",
                "inputMode": "passwordbox",
                "isConfidential": true,
                "validation": {
                  "isRequired": false,
                  "dataType": "string"
                },
                "values": {
                  "inputId": "passwordInput",
                  "defaultValue": "",
                  "isDisabled": true
                }
              }
            ]
          }
        ],
        "helpMarkDown": ""
      }
    }
  ]
}
